<GridPane alignment="CENTER" hgap="10.0" prefHeight="432.0" prefWidth="799.0" styleClass="root" vgap="10.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="fxmlPages.HomePageController">
  <Text id="welcome-text" text="Welcome to Clinic Assistance" GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.rowIndex="0" />
  <Label text="User Name:" GridPane.columnIndex="0" GridPane.rowIndex="1" />
  <TextField fx:id="uname" GridPane.columnIndex="1" GridPane.rowIndex="1" />
  <Label text="Password:" GridPane.columnIndex="0" GridPane.rowIndex="2" />
  <PasswordField fx:id="passwordField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
  <HBox alignment="BOTTOM_RIGHT" spacing="10.0" GridPane.columnIndex="0" GridPane.rowIndex="4">
    <Button onAction="#handleSubmitButtonAction" text="Submit" />
  </HBox>
  <!--    <HBox spacing="10" alignment="bottom_right" 
      GridPane.columnIndex="1" GridPane.rowIndex="4">
      <Button text="New User" 
       onAction="#handleNewUserButtonAction" />
  </HBox> -->
  <HBox alignment="BOTTOM_RIGHT" spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
    <Button onAction="#handleExitAppButtonAction" text="Exit Application" />
  </HBox>
  <Text fx:id="actiontarget" GridPane.columnIndex="1" GridPane.rowIndex="6" />
  <padding>
    <Insets bottom="10.0" left="25.0" right="25.0" top="25.0" />
  </padding>
  <rowConstraints>
    <RowConstraints maxHeight="46.0" prefHeight="46.0" />
    <RowConstraints maxHeight="22.0" prefHeight="22.0" />
    <RowConstraints maxHeight="22.0" prefHeight="22.0" />
    <RowConstraints maxHeight="0.0" prefHeight="0.0" />
    <RowConstraints maxHeight="19.0" prefHeight="19.0" />
    <RowConstraints maxHeight="0.0" prefHeight="0.0" />
    <RowConstraints maxHeight="16.0" prefHeight="16.0" />
  </rowConstraints>
  <stylesheets>
    <URL value="@Login.css" />
  </stylesheets>
</GridPane>
